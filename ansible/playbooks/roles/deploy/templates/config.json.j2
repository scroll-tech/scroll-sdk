{
    "prover_name": "{{ansible_facts['hostname']}}-docker",
    "keystore_path": "/prover/volume/keystore.json",
    "keystore_password": "prover-pwd",
    "db_path": "/prover/volume/bbolt_db",
    "proof_type": {{ proof_type }},
    "low_version_circuit": {
        "hard_fork_name": "bernoulli",
        "params_path": "/prover/volume/params",
        "assets_path": "/prover/volume/assets_bernoulli"
    },
    "high_version_circuit": {
        "hard_fork_name": "curie",
        "params_path": "/prover/volume/params",
        "assets_path": "/prover/volume/assets_curie"
    },
    "coordinator": {
        "base_url": "{{base_url}}",
        "retry_count": 10,
        "retry_wait_time_sec": 10,
        "connection_timeout_sec": 200
    },
    "l2geth": {
        "endpoint": "{{rpc_endpoint}}"
    }
}